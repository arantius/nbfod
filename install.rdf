<?xml version="1.0" encoding="UTF-8"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:id>nbfod@arantius.com</em:id>
    <em:name>NewsBlur Frame Options Defeater</em:name>
    <em:description>Allows all sites to be loaded into NewsBlur, despite X-Frame-Options frame busting settings.</em:description>
    <em:version>2</em:version>
    <em:type>2</em:type>
    <em:bootstrap>true</em:bootstrap>
    <em:targetApplication>
     <Description>
      <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id> <!--Firefox-->
      <em:minVersion>28</em:minVersion>
      <em:maxVersion>30.*</em:maxVersion>
     </Description>
    </em:targetApplication>
  </Description>
</RDF>
